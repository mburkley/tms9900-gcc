+;;-------------------------------------
+(define_insn "addhi3"
+  [(set (match_operand:HI 0 "nonimmediate_operand"          "=rR>,   Q,      r,rR>,Q")
+	(plus:HI (match_operand:HI 1 "nonimmediate_operand" "%0,     0,      0,0,  0")
