+    emit_insn(gen_extendqihi2 (operands[1], sign));
+    // emit_move_insn(sign, operands[1]);
+    emit_insn(gen_extendqihi2 (operands[2], operands[2]));
+    emit_insn(gen_xorhi3(sign, sign, operands[2]));
