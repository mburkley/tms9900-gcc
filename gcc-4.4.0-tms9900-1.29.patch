+  if (from == ARG_POINTER_REGNUM && to == HARD_SP_REGNUM)
+  {
+    ret =(tms9900_get_saved_reg_size()+
+           get_frame_size ());
+  }
+  if (from == FRAME_POINTER_REGNUM && to == HARD_SP_REGNUM)
+  {
+    ret = 0;
+  }
+  if (from == ARG_POINTER_REGNUM && to == FRAME_POINTER_REGNUM)
+  {
+    ret =(tms9900_get_saved_reg_size()+
+           get_frame_size ());
+  }
