+        if (INTVAL(operands[2]) == 1)
+	  return("inc  %0");
+        else if (INTVAL(operands[2]) == -1)
+          return("dec  %0");
+        else if (INTVAL(operands[2]) == 2)
+          return("inct %0");
+        else if (INTVAL(operands[2]) == -2)
