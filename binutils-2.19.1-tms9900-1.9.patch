+static void
+tms9900_bss (int unused ATTRIBUTE_UNUSED)
+{
+  expressionS exp;
+  expression(&exp);
+  if(exp.X_op == O_constant)
+    {
+      char* p = frag_more(exp.X_add_number);
+      memset(p, 0, exp.X_add_number);      
+    }
+  else
+    {
+      as_bad (_("junk at end of line, first unrecognized character is `%c'"),
+              *input_line_pointer);
+    }
+}
